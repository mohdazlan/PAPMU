{
  "expo": {
    "name": "PAPMU",
    "slug": "PAPMU",
    "version": "1.0.0",                // Semantic version for users (major.minor.patch)
    "orientation": "portrait",
    "icon": "./assets/images/logo_PAPMU.png",
    "scheme": "papmu",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,

    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },

    "android": {
      "package": "com.azlan.papmu", // ðŸ‘ˆ unique Android package name
      "versionCode": 1,                // ðŸ‘ˆ increment by 1 for every Play Store update
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo_PAPMU.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },

    "ios": {
      "bundleIdentifier": "com.azlan.papmu", // ðŸ‘ˆ unique iOS bundle ID
      "buildNumber": "1.0.0",                   // ðŸ‘ˆ increment for every App Store update
      "supportsTablet": true
    },

    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo_PAPMU.png"
    },

    "plugins": ["expo-router"],

    "experiments": {
      "typedRoutes": true
    },

    "extra": {
      "router": {},
      "eas": {
        "projectId": "778e1555-f7ee-47b7-bba6-9cb678f7dd0d"
      }
    }
  }
}

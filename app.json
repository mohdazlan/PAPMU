{
  "expo": {
    "name": "PAPMU",
    "slug": "PAPMU",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo_PAPMU.png",
    "scheme": "papmu",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo_PAPMU.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo_PAPMU.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "778e1555-f7ee-47b7-bba6-9cb678f7dd0d"
      }
    }
  }
}
